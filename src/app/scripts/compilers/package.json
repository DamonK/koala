[
	{
		"name": "less",
		"display": "LESS",
		"main": "LessCompiler.js",
		"version": "1.0.0",

		"maintainers": {
			"name": "Ethan Lai",
			"email": "lain.z.q@gmail.com",
			"web": "http://oklai.name",
			"project": "http://github.com/oklai/koala"
		},

		"contributors": [
			{"name": "Ziad Khoury","web": "https://github.com/zaygraveyard"}
		],

		"isExtension": true,

		"file_types": {
			"extension": "less",
			"output": "css",
			"icon": "../assets/img/filetypes/less.png",
			"category": "style"
		},

		"options": [
			{
				"type": "checkbox",
				"name": "lineComments",
				"display": "Line Comments",
				"default": false
			},
			{
				"type": "checkbox",
				"name": "debugInfo",
				"display": "Debug Info",
				"default": false
			},
			{
				"type": "checkbox",
				"name": "strictMath",
				"display": "Strict Math",
				"default": true
			},
			{
				"type": "checkbox",
				"name": "strictUnits",
				"display": "Strict Units",
				"default": false
			},
			{
				"type": "droplist",
				"name": "outputStyle",
				"display": "Output Style",
				"items": [
					{
						"value": "",
						"text": "normal"
					},
					"compress",
					"yuicompress"
				],
				"default": ""
			}
		],

		"advanced": [
			{
				"type": "checkbox",
				"name": "useCommand",
				"display": "Use the System LESS compiler",
				"default": false
			},
			{
				"type": "text",
				"name": "commandPath",
				"display": "Use the LESS executable at this path",
				"default": "",
				"placeholder": "Default: lessc",
				"depend": "useCommand"
			}
		],

		"project_settings": "../settings/koala-config-of-less.json",

		"libs": "less@1.4.0"
	},
	{
		"name": "sass",
		"display": "Sass",
		"main": "SassCompiler.js",
		"version": "1.0.0",

		"file_types": [
			{
				"extension": "sass",
				"output": "css",
				"icon": "../assets/img/filetypes/sass.png",
				"category": "style",
				"watch": true
			},
			{
				"extension": "scss",
				"output": "css",
				"icon": "../assets/img/filetypes/scss.png",
				"category": "style",
				"watch": true
			}
		],

		"options": [
			{
				"type": "checkbox",
				"name": "compass",
				"display": "Compass Mode",
				"default": false
			},
			{
				"type": "checkbox",
				"name": "lineComments",
				"display": "Line Comments",
				"default": false
			},
			{
				"type": "checkbox",
				"name": "debugInfo",
				"display": "Debug Info",
				"default": false
			},
			{
				"type": "checkbox",
				"name": "unixNewlines",
				"display": "Unix Newlines",
				"default": false
			},
			{
				"type": "droplist",
				"name": "outputStyle",
				"display": "Output Style",
				"items": [
					"nested",
					"expanded",
					"compact",
					"compressed"
				],
				"default": "nested"
			}
		],

		"advanced": [
			{
				"type": "checkbox",
				"name": "useCommand",
				"display": "Use the System Sass compiler",
				"default": false
			},
			{
				"type": "text",
				"name": "commandPath",
				"display": "Use the Sass executable at this path",
				"default": "",
				"placeholder": "Default: sass",
				"depend": "useCommand"
			}
		],

		"project_settings": "../settings/koala-config-of-sass.json",

		"libs": ["sass@3.2.9"]
	},
