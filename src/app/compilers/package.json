[
	{
		"name": "less",
		"display": "LESS",
		"main": "LessCompiler.js",
		"version": "1.0.0",

		"maintainers": {
			"name": "Ethan Lai",
			"email": "lain.z.q@gmail.com",
			"web": "http://oklai.name",
			"project": "http://github.com/oklai/koala"
		},

		"file_types": {
			"extension": "less",
			"output": "css",
			"icon": "../assets/img/filetypes/less.png",
			"category": "style"
		},

		"options": [
			{
				"name": "lineComments",
				"display": "Line Comments",
				"type": "single",
				"default": false
			},
			{
				"name": "debugInfo",
				"display": "Debug Info",
				"type": "single",
				"default": false
			},
			{
				"name": "strictMath",
				"display": "Strict Math",
				"type": "single",
				"default": true
			},
			{
				"name": "strictUnits",
				"display": "Strict Units",
				"type": "single",
				"default": false
			},
			{
				"name": "outputStyle",
				"display": "Output Style",
				"type": "multiple",
				"items": [
					{
						"value": "",
						"text": "normal"
					},
					"compress",
					"yuicompress"
				],
				"default": ""
			}
		],

		"commands": "lessc",
		"libs": "less@1.4.0"
	},
	{
		"name": "sass",
		"display": "Sass",
		"main": "SassCompiler.js",
		"version": "1.0.0",

		"file_types": [
			{
				"extension": "sass",
				"output": "css",
				"icon": "../assets/img/filetypes/sass.png",
				"category": "style"
			},
			{
				"extension": "scss",
				"output": "css",
				"icon": "../assets/img/filetypes/scss.png",
				"category": "style"
			}
		],

		"options": [
			{
				"name": "compass",
				"display": "Compass Mode",
				"type": "single",
				"default": false
			},
			{
				"name": "lineComments",
				"display": "Line Comments",
				"type": "single",
				"default": false
			},
			{
				"name": "debugInfo",
				"display": "Debug Info",
				"type": "single",
				"default": false
			},
			{
				"name": "unixNewlines",
				"display": "Unix Newlines",
				"type": "single",
				"default": false
			},
			{
				"name": "outputStyle",
				"display": "Output Style",
				"type": "multiple",
				"items": [
					"nested",
					"expanded",
					"compact",
					"compressed"
				],
				"default": "nested"
			}
		],

		"commands": ["sass", "compass"],
		"libs": ["sass@3.2.9", "compass@0.12.2"]
	},
	{
		"name": "coffeescript",
		"display": "CoffeeScript",
		"main": "CoffeeScriptCompiler.js",
		"version": "1.0.0",

		"file_types": {
			"name": "coffeescript",
			"extension": "coffee",
			"output": "js",
			"icon": "../assets/img/filetypes/coffee.png",
			"category": "script"
		},

		"options": [
			{
				"name": "bare",
				"display": "Bare",
				"type": "single",
				"default": false
			},
			{
				"name": "literate",
				"display": "Literate",
				"type": "single",
				"default": false
			}
		],

		"commands": "coffee",
		"libs": "coffee-script@1.6.3"
	},
	{
		"name": "dust",
		"display": "Dust",
		"main": "DustCompiler.js",
		"version": "1.0.0",

		"maintainers": {
			"name": "Ziad Khoury",
			"email": "",
			"web": "https://github.com/zaygraveyard",
			"project": "http://github.com/oklai/koala"
		},

		"file_types": {
			"extension": "dust",
			"output": "jst",
			"icon": "../assets/img/filetypes/dust.png",
			"category": "template"
		},

		"commands": "dustc",
		"libs": "dustjs-linkedin@1.2.3"
	}
]
